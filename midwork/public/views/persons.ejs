<h1>Users Stored</h1>
<% if (userinfos.length) { %>
  <ul>
    <% userinfos.forEach(function(userinfo){ %>
      <li>
        <div class='name'>
          name:<div class="ans"><%= userinfo['name'] %></div><br />
          email:<div class="ans"><a href="mailto:<%= userinfo['email']%>?&subject=age8artwork回答您的問題&body=hi, <%= userinfo['name'] %>,關於您在我的網站問的問題"><%= userinfo['email'] %></a></div><br />
          problem:<br /><div class="ans"><%= userinfo['problem'] %></div>
        </div>
        <a href="" class='read' rel=<%= userinfo['_id']%>>VIEW</a>
        <a href="" class='delete' rel=<%= userinfo['_id']%>>DEL</a>
        <a href="" class='update' rel=<%= userinfo['_id']%>>EDIT</a>
      </li>
    <% }) %>
  </ul>
<% } %>
